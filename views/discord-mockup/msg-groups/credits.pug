#credits.messages-group
    .header Development-Credits
    .messages
        +msg('assets/img/logo.png', 'Octave')
            +embed-desc('#9571d3')
                .title Melmsie
                img.thumb(src='assets/img/melmsie.png', alt='Melmsie owns Octave')
                .text: p Owner of Octave and Dank Memer, in charge of marketing and creative decisions.
            +embed-desc('#ff69b4')
                .title Kodehawa
                img.thumb(src='assets/img/kode.png', alt='Kode works on Octave')
                .text: p Head developer of Octave, also owns the popular bot 
                        a(href='https://mantaro.site/' target='_blank') Mantaro
            +embed-desc('#1e90ff')
                .title Devoxin
                img.thumb(src='assets/img/devoxin.png', alt='Devoxin works on Octave')
                .text: p Worked on lots of large bots including Dank Memer, Kawaiibot, BoobBot, and now a developer for Octave.
            +embed-desc('#ca2d36')
                .title Aetheryx
                img.thumb(src='assets/img/aetheryx.png', alt='Aetheryx Dank Memer')
                .text: p Head of Development on 
                        a(href='https://dankmemer.lol/' target='_blank') Dank Memer
            +embed-desc
                .title Avarel
                img.thumb(src='assets/img/avarel.jpg', alt='Avarel owned Gnar')
                .text: p Previous owner/developer on Octave, formerly known as Gnar
            +embed-desc('#8A0303')
                .title Xevryll
                img.thumb(src='assets/img/xev.png', alt='Xev owned Gnar')
                .text: p Previous owner/developer on Octave, formerly known as Gnar